@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager



<ul class="navbar-nav">
    @if (SignInManager.IsSignedIn(User))
    {
        <li class="nav-item" style="margin-top:5px">
                <a asp-area="Identity"
                    asp-page="/Account/Manage/Index" title="Manage" >Profile</a>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register">REGISTO</a>
        </li>
        <li class="nav-item col-5">
            <a class="btn btn-outline-primary ms-2 me-2" asp-area="Identity" asp-page="/Account/Login">ENTRAR</a>
        </li>
    }
</ul>